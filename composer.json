{
    "name": "piotrmus/yii2-chartjs2-widget",
    "description": "ChartJs2 widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "piotrmus",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "chartjs2"
    ],
    "homepage": "http://codehat.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Piotr Musia≈Ç",
            "email": "piotr.mus@gmail.com",
            "homepage": "http://codehat.pl",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/piotrmus/yii2-chartjs2-widget/issues",
        "source": "https://github.com/piotrmus/yii2-chartjs2-widget"
    },
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/chartjs": "2.1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "piotrmus\\chartjs2\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}